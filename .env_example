# Copy this file to .env and fill in values as needed
# mv .env_example .env

# External services
OPENAI_API_KEY=
WANDB_API_KEY=

# Weave project (org/project). Default: "pierg-org/codreamer"
WEAVE_PROJECT=pierg-org/codreamer

# ART model configuration
ART_PROJECT=codreamer
ART_MODEL_NAME=model
ART_BASE_MODEL=Qwen/Qwen2.5-14B-Instruct

# Optional webhook for frontend updates
FRONTEND_URL=

# Optional CLI overrides
# NUM_ITERS=3
# DEPTH=5
# RUN_ID=my-run
